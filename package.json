{
  "name": "micro-credential-aggregator",
  "version": "1.0.0",
  "description": "Automated certificate collection from Gmail using Google OAuth and Supabase storage",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "frontend:install": "cd frontend && npm install",
    "frontend:dev": "cd frontend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "dev:all": "concurrently \"npm run dev\" \"npm run frontend:dev\"",
    "build": "npm run frontend:build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "certificates",
    "gmail-api",
    "oauth2",
    "supabase",
    "gemini-ai",
    "coursera",
    "automation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.5.0",
    "googleapis": "^126.0.1",
    "@supabase/supabase-js": "^2.38.0",
    "uuid": "^9.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}